public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    if(passedExam && passedAssignments == true){
        return true;
        }else if(passedAssignments && passedProject == true){
            return true;
            }else if(passedExam && passedProject == true){
                return true;
                }else{
                return false;
                }
}